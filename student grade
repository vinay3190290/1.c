#include<stdio.h>
int main(){
	int a,b,c,d,e,f,g;
	printf("enter first subject marks:");
	scanf("%d",&a);
	printf("enter scond subject marks:");
	scanf("%d",&b);
	printf("enter third subject marks:");
	scanf("%d",&c);
	printf("enter 4th subject marks:");
	scanf("%d",&d);
	e=(a+b+c+d)/4;
	printf("the score is %d",e);
	if(e>75)
	{
		printf("grade is distinction");
	}
	else if (e>=60&&e<75)
	{
		printf("the grade is 1st class");
	}
	else if (e>=50&&e<=60)
	{
		printf("the grade is 2nd class");
	}
	else if (e>=40&&e<=50)
	{
		printf("the grade is 3rd class");
	}
	else
	{
		printf("fail");
	}
}
